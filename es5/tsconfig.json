{
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": false,
      "target": "ES5",
      "declaration": true,
      "outDir": "./",
      "declarationDir": "./",
      "downlevelIteration": true,
      "lib": ["es2020"]
    },
    "include": ["../src/**/*"]
}